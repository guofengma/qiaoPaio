/*列表属性  */
.radiusTopLeft{
  margin-top: 20rpx;
  border-radius: 10rpx 10rpx 0 0;
}
.radiusBottomRight{
  border-radius: 0 0 10rpx 10rpx;
  border-bottom: none;
}
.back{
  border-bottom: 1rpx solid #ebebeb;
}
.marginTop{
  margin-top: 20rpx;
}
.moreFrame{
  width: 100%;
  display: flex;
  padding: 10rpx 20rpx;
  box-sizing: border-box;
  background: #fff;
  font-size: 32rpx;
  align-items: center;
}
.moreFrame .moreImg{
  flex: 0 0 30rpx;
  height: 30rpx;
  margin-right: 10rpx;
}
.moreFrame .moreImg image{
   vertical-align: top; 
  display: inline-block;
  width: 100%;
  height: 100%;
}
.moreFrame .moreName{
  flex:1;
  width:50%;
  display: flex;
  align-items: center;
}
.moreFrame .moreName .moreText{
  flex: 1;
}
.moreFrame .moreIocn{
  flex: 1;
  text-align: right;
  display: flex;
  flex-direction: row-reverse;
  align-items: center;
  vertical-align: middle
}
.moreFrame .moreIocn .price{
  flex: 1;
}
.moreFrame .moreIocn .price text{
  display: inherit;
}
.moreFrame .moreIocn .iocn{
  flex: 0 0 30rpx;
  display: inline-block;
} 
.moreFrame .moreIocn .iocn .iconfont{
  font-size: 32rpx;
  margin: -10rpx 0 0 0;  
}
.moreFrame .moreIocn .price{
  display: flex;
  align-items: center;
  justify-content: flex-end;
}
.price .image{
  flex: 0 0 50rpx;
  width:50rpx;
  height: 50rpx;
  margin-right: 10rpx;
}
.price .image image{
  border-radius: 50%;
}
/*列表属性  */

/*文章列表的内容有可能出现图片，需要隐藏*/
.item-info-box .info-con image{
  display: none !important;
}