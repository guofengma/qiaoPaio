/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.nav_hei{
  height:3rpx;
  background-color:#ccc;
}
.navbar{ 
 flex: none; 
 display: flex; 
 background: #fff; 
 font-size: 32rpx;
} 
.navbar .item{ 
 position: relative; 
 flex: auto; 
 text-align: center; 
 line-height: 80rpx; 
} 
.navbar .item.active{ 
 color:#da251c; 
} 

/*公共样式  */
.none{
  display: none;
}
.mt20{
  margin-top: 20rpx;
}
.mb10{
  margin-bottom: 10rpx;
}
.mb20{
	margin-bottom: 20rpx;
}
.mb30{
  margin-bottom: 30rpx;
}
.mb40{
	margin-bottom: 40rpx;
}
.fl{
  float: left;
}
.fr{
  float: right;
}
.clearfix:before,.clearfix:after{
  content: '';
  display: block;
  clear: both;
  height: 0;
  font-size: 0;
  overflow: hidden;
}

page{
	font-size: 28rpx;
}

.noData{
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  background: #fff;
  font-size: 28rpx;
}

.flex{
	display: flex;
	display: -webkit-flex;
}
.center-flex{
  display: flex;
  display: -webkit-flex;
  align-items: center;
}


/*黄色按钮*/
.yellow-btn{
  background: #fddc35;
  color: #32383b;
  border-radius: 10rpx;
  border:1rpx solid #fddc35;
}

/*灰色按钮*/
.grey-btn{
  background: #e9e9e8;
  border:1rpx solid #dedddd;
  color: #2c2c2c;
  border-radius: 10rpx;
}
